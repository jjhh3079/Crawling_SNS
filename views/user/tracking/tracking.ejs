                                        <tr>
                                            <th>운송장 번호</th>
                                            <th>택배사</th>
                                            <th>마지막 날짜</th>
                                            <th>상태</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <%tracking.forEach((item)=>{%>
                                            <tr>
                                                <td>
                                                    <%=item.Tracking_Number%>
                                                </td>
                                                <td>
                                                    <%=item.Tracking_Company%>
                                                </td>
                                                <td>
                                                    <%=new Date(item.Tracking_Date).toLocaleString()%>
                                                </td>
                                                <td>
                                                    <%=item.Tracking_Status%>
                                                </td>
                                            </tr>
                                    <%})%>
                                    </tbody>
                                </table>
                            </div>
                    </div>
                        </div>
                    </div>
                </div>
