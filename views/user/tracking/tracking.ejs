                                      <option value="04">CJ대한통운</option>
                                      <option value="46">CU편의점택배</option>
                                      <option value="24">CVSnet편의점택배</option>
                                      <option value="10">KGB택배</option>
                                      <option value="30">KGL네트웍스</option>
                                      <option value="39">KG로지스</option>
                                      <option value="44">SLX</option>
                                      <option value="99">롯데글로벌 로지스</option>
                                      <option value="37">범한판토스</option>
                                      <option value="29">에어보이익스프레스</option>
                                      <option value="35">포시즌익스프레스</option>
                                      <option value="38">APEX(ECMS Express)</option>
                                      <option value="42">CJ대한통운 국제특송</option>
                                      <option value="13">DHL</option>
                                      <option value="33">DHL Global Mail</option>
                                      <option value="12">EMS</option>
                                      <option value="21">Fedex</option>
                                      <option value="41">GSI Express</option>
                                      <option value="28">GSMNtoN(인로스)</option>
                                      <option value="34">i-Parcel</option>
                                      <option value="25">TNT Express</option>
                                      <option value="27">TPL</option>
                                      <option value="14">UPS</option>
                                      <option value="26">USPS</option>
                                  </select>
